+        code: true,
+        name: true,
+        category: true,
+        goals: true,
+        durationMin: true,
+        price: true,
+        ratingAvg: true,
+        bookedCount: true,
+        imageUrl: true,
+        tag: true,
+      },
+      orderBy: { id: 'asc' },
+    })
+
+    return rows.map((s) => ({
+      id: s.code,
+      dbId: s.id,
+      name: s.name,
+      cat: s.category ?? 'health',
+      goal: Array.isArray(s.goals) ? s.goals : [],
+      duration: s.durationMin,
+      price: s.price,
+      rating: s.ratingAvg,
+      booked: s.bookedCount,
+      img: s.imageUrl,
+      tag: s.tag ?? 'Spa',
+    }))
+  }
+
+  async listBranches(): Promise<BranchResponseDto[]> {
+    const rows = await this.prisma.branch.findMany({
+      where: { isActive: true },
+      select: { id: true, code: true, name: true, address: true, phone: true },
+      orderBy: { id: 'asc' },
+    })
