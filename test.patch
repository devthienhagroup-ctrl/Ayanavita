+++ b/backend/ayanavitabackend/app/api/src/main.ts
@@ -7,50 +7,52 @@ import { AllExceptionsFilter } from "./common/filters/all-exceptions.filter";

 async function bootstrap() {
   const app = await NestFactory.create(AppModule, {
     logger: ["log", "error", "warn", "debug", "verbose"],
   });

   // ‚úÖ CORS ‚Äì chu·∫©n cho c·∫£ production + admin + dev
   const allow = new Set<string>([
     // üåç Production main domain
     "http://ayanavita-germany.com",
     "https://ayanavita-germany.com",

     // üîê Admin subdomain
     "http://admin.ayanavita.com",
     "https://admin.ayanavita.com",

     // üß™ Dev local
     "http://localhost:5173",
     "http://127.0.0.1:5173",
     "http://localhost:5174",
     "http://127.0.0.1:5174",
     "http://localhost:5175",
     "http://127.0.0.1:5175",
     "http://localhost:5176",
     "http://127.0.0.1:5176",
+    "http://localhost:5178",
+    "http://127.0.0.1:5178",
     "http://localhost:5050",
     "http://127.0.0.1:5050",
   ]);

   app.enableCors({
     origin: (origin, callback) => {
       // Cho ph√©p server-to-server ho·∫∑c curl
       if (!origin) return callback(null, true);

       if (allow.has(origin)) {
         return callback(null, true);
       }

       console.error(`‚ùå CORS blocked for origin: ${origin}`);
       return callback(new Error(`CORS blocked for origin: ${origin}`), false);
     },
     credentials: true,
     methods: ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"],
     allowedHeaders: ["Content-Type", "Authorization"],
     optionsSuccessStatus: 204,
   });

   // ‚úÖ Global Exception Filter
   app.useGlobalFilters(new AllExceptionsFilter());

